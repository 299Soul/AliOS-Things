.picker-mask {
  width: 100%;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
  z-index: 1000;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.picker-contenter {
  position: relative;
  width: 80%;
  background-color: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-radius: 4rpx;
}

.pick-button-row {
  display: flex;
  flex-direction: row;
  margin-left: 200rpx;
}

.picker-button {
  background: none;
  border: none;
  color: rgb(0, 162, 255);
  width: 150rpx;
  height: 100rpx;
  font-size: 32rpx;
}

.picker-button-hover {
  background: none;
  border: 1rpx solid rgb(0, 162, 255);
  color: rgb(168, 168, 168);
  width: 150rpx;
  height: 100rpx;
}

.wifi-picker {
  width: 80%;
}